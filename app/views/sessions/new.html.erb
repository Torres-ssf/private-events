<% provide(:title, 'Log in') %>
<h1 class="page-heading" >Log in</h1>

<div class="row">
  <div class="form-container">
    <%= form_for(:session, html: {class: "signup-form"}) do |f| %>

      <%= f.label :email %>
      <%= f.email_field :email %>

      <%= f.submit "Log in", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>
